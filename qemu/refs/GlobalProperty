<mbr r='GlobalProperty::value' o='128' t='const char *'/>
<mbr r='GlobalProperty::user_provided' o='192' t='_Bool'/>
<mbr r='GlobalProperty::used' o='200' t='_Bool'/>
<mbr r='GlobalProperty::errp' o='256' t='Error **'/>

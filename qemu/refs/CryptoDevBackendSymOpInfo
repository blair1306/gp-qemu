<mbr r='CryptoDevBackendSymOpInfo::digest_result_len' o='192' t='uint32_t'/>
<mbr r='CryptoDevBackendSymOpInfo::hash_start_src_offset' o='224' t='uint32_t'/>
<mbr r='CryptoDevBackendSymOpInfo::cipher_start_src_offset' o='256' t='uint32_t'/>
<mbr r='CryptoDevBackendSymOpInfo::len_to_hash' o='288' t='uint32_t'/>
<mbr r='CryptoDevBackendSymOpInfo::len_to_cipher' o='320' t='uint32_t'/>
